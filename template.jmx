            <JSR223Timer guiclass="TestBeanGUI" testclass="JSR223Timer" testname="JSR223 Timer" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">/**
 * PACING
 * the interval pacing solution.
 * 
 * Radomization can be added by using this code as sampler to calculate 
 * boundaries for different timers such as Gaussian or Poisson Random timer
 * 
 * @author dinanathbasumatary
 */

def d = new Date() // initialize current date
def thread = ctx.getThreadGroup().getName()

if (vars.get(&quot;${thread}_Pacing&quot;) != null){
	pacing = Long.parseLong(vars.get(&quot;${thread}_Pacing&quot;)) //parse the pacing from variable
	
	try {
		String startTime = vars.get(&quot;pacingStartTime&quot;)
		
		if (startTime != null) {
			def diff = d.getTime() - Long.parseLong(startTime)
			def sleep = pacing &gt; diff ? pacing - diff : 0 // logic to calculate pacing
			log.info(&quot;[ ${thread} - Pacing: ${pacing}ms, Remaining time: ${sleep}ms ]&quot;)
			
			return sleep
		}
		else {
			log.info(&quot;[ ${thread} - Pacing: initialized! ]&quot;)
		}
		return 0
	}
	catch (NumberFormatException e) {
		return 1000
		log.warn(&quot;[ ${thread} - Pacing: unable to parse start time ]&quot;, e)
		throw e
	}
	catch (Exception e) {
		return 1000
		log.warn(&quot;[ ${thread} - Pacing: Failed to calculate pacing ]&quot;, e)
		
    		throw e
		//SampleResult.setSuccessful(false)
	}
}
else {
	log.error(&quot;[ ${thread} - Pacing: a jmeter variable named &apos;&lt;ThreadGroupName&gt;_Pacing&apos; is expected. eg: &apos;${thread}_Pacing&apos;. Please add it before proceeding! ]&quot;)
	log.error(&quot;[ ${thread} - Pacing: Stopping the test. ]&quot;)
	ctx.getEngine().askThreadsToStop() // stop test if its not defined.
	return 0
}
</stringProp>
            </JSR223Timer>
